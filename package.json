{
  "name": "elysium-quiz",
  "version": "1.0.0",
  "description": "Emotional realm matching quiz for Elysium",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:congruency": "vitest run tests/congruency",
    "test:semantic": "vitest run tests/semantic",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^28.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.18"
  }
}
